                    android:layout_marginStart="110dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/amaranth_bold"
                        android:paddingStart="20dp"
                        android:paddingTop="5dp"
                        android:text="Shree Rama Aarti"
                        android:textColor="#633202"
                        android:textSize="20sp">

                    </TextView>

                    <TextView
                        android:layout_width="250dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_regular"
                        android:paddingTop="5dp"
                        android:text="श्री रामचन्द्र कृपालु भजु मन"
                        android:textColor="#b7620e"
                        android:textSize="18dp">

                    </TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ganeshji"></ImageView>

                    <TextView
                        android:layout_width="230dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_regular"
                        android:paddingLeft="0dp"
                        android:paddingTop="5dp"
                        android:text="Shri Rama Chandra Kripalu"
                        android:textColor="#b7620e"
                        android:textSize="16sp">

                    </TextView>


                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/carda5"
                android:layout_width="350dp"
                android:layout_height="125dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/shadow"
                app:cardBackgroundColor="#FECA6B"
                app:cardCornerRadius="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="110dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/amaranth_bold"
                        android:paddingStart="0dp"
                        android:paddingTop="5dp"
                        android:text="Shri Ramayan Ji Aarti"
                        android:textColor="#633202"
                        android:textSize="20sp">

                    </TextView>

                    <TextView
                        android:layout_width="250dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_regular"
                        android:paddingLeft="3dp"
                        android:paddingTop="5dp"
                        android:text="आरती श्री रामायण जी की"
                        android:textColor="#b7620e"
                        android:textSize="18sp">

                    </TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ganeshji"></ImageView>

                    <TextView
                        android:layout_width="230dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_regular"
                        android:paddingStart="5dp"
                        android:paddingTop="5dp"
                        android:text="Aarti Shri Ramayan Ji Ki"
                        android:textColor="#b7620e"
                        android:textSize="17sp">

                    </TextView>


                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/carda6"
                android:layout_width="350dp"
                android:layout_height="125dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:background="@drawable/shadow"
                app:cardBackgroundColor="#FECA6B"
                app:cardCornerRadius="25dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="110dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/amaranth_bold"
                        android:paddingStart="25dp"
                        android:paddingTop="5dp"
                        android:text="Lord Shiva Aarti"
                        android:textColor="#633202"
                        android:textSize="20sp">

                    </TextView>

                    <TextView
                        android:layout_width="250dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_regular"
                        android:paddingStart="0dp"
                        android:paddingTop="5dp"
                        android:text="ॐ जय शिव ओंकारा,स्वामी जय"
                        android:textColor="#b7620e"
                        android:textSize="18sp">

                    </TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ganeshji"></ImageView>

                    <TextView
                        android:layout_width="230dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/poppins_regular"
                        android:paddingLeft="5dp"
                        android:paddingTop="5dp"
                        android:text="Om Jai Shiv Omkara"
                        android:textColor="#b7620e"
                        android:textSize="17sp">

                    </TextView>


                </LinearLayout>

            </androidx.cardview.widget.CardView>



        </LinearLayout>
    </ScrollView>

</RelativeLayout>